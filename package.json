{
  "name": "eier-speck-notification",
  "version": "1.0.0",
  "description": "Send a whatsapp notification when the ticket pre-sale of the Eier mit Speck festival starts",
  "scripts": {
    "dev": "parcel watch src/index.js --target node",
    "start": "node dist/index.js",
    "build": "parcel build src/index.js --target node --no-source-maps --no-content-hash"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jkonze/eier-speck-notification.git"
  },
  "keywords": [
    "twilio",
    "rss",
    "festival"
  ],
  "author": "Johannes Konze <johannes@konze.it>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jkonze/eier-speck-notification/issues"
  },
  "homepage": "https://github.com/jkonze/eier-speck-notification#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "parcel-bundler": "^1.12.4",
    "rss-parser": "^3.7.3",
    "twilio": "^3.38.0"
  }
}
